  "description": "OpenseaCustomMarketplace",
  "main": "index.js",
  "scripts": {
    "test": "truffle test ./test/*",
    "start-blank": "rm -rf client/src && cp -rf barebones/src client/",
    "client": "cd client && set 'NODE_ENV=production' && npm run start",
    "console": "truffle console --network local",
    "migrate:local": "truffle migrate --reset --network local",
    "migrate:ropsten": "truffle migrate --reset --network ropsten",
    "migrate:rinkeby": "truffle migrate --reset --network rinkeby",
    "migrate:binancetest": "truffle migrate --reset --network binancetest",
    "migrate:bsc": "truffle migrate --reset --network bsc",
    "migrate:gather": "truffle migrate --reset --network gather"
  },
